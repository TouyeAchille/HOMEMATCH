generate_listings:
	@echo "----------------------------------------"
	@echo "Generating listings from input prompt..."
	@echo "Input prompt: $$(cat input_prompt.txt)"
	@echo "Output directory: ~/HOMEMATCH/data_storage"
	@echo "Running Python script to generate listings..."
	@echo "----------------------------------------"
	python generate_listings.py --input_text_prompt "$$(cat input_prompt.txt)" --dirpath_to_save_output_artifact ~/HOMEMATCH/data_storage
		
